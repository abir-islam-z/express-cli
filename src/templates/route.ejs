import { Router } from 'express';
import { <%= m_name %>Controller } from './<%= name %>.controller';

const router = Router();

router.post(
  '/',
  <%= m_name %>Controller.create,
);

router.get(
  '/',
  <%= m_name %>Controller.findAll,
);

router.get(
  '/:id',
  <%= m_name %>Controller.findOne,
);

router.patch(
  '/:id',
  <%= m_name %>Controller.update,
);

router.delete(
  '/:id',
  <%= m_name %>Controller.remove,
);


export const <%= m_name %>Routes = router;
